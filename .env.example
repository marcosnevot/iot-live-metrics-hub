# Database configuration (local dev example)
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=replace-with-local-db-password
DB_NAME=postgres

# MQTT broker (local dev example)
MQTT_BROKER_URL=mqtt://127.0.0.1:1883

# JWT configuration (local dev example)
JWT_SECRET=replace-with-local-jwt-secret
JWT_EXPIRES_IN=1h

# Human users (local dev example)
ADMIN_USERNAME=admin@local.test
ADMIN_PASSWORD=replace-with-admin-password
ANALYST_USERNAME=analyst@local.test
ANALYST_PASSWORD=replace-with-analyst-password

LOG_LEVEL=info
